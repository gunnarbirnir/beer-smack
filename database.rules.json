{
  "rules": {
    ".read": true,
    ".write": false,
    "rooms": {
      ".write": true,
      "$room_id": {
        "users": {
          "$user_id": {
            "ratings": {
              "$beer_id": {
                ".validate": "newData.isNumber() && newData.val()>=0 && newData.val()<=10"
              }
            }
          }
        }
      }
    }
  }
}
